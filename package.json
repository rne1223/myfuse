{
  "name": "myfuse",
  "displayName": "MyFuse",
  "description": "Plugin to create Fuses for Black Magick Design's Fusion and Davinci Resolve",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.53.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rne1223"
  },
  "icon": "images/logo.png",
  "categories": [
    "Snippets",
    "Other"
  ],
  "keywords": [
    "Davinic",
    "Black Magick",
    "Fusion",
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:myfuse.createSimpleFuse"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "myfuse.createSimpleFuse",
        "category": "MyFuse",
        "title": "Create Simple Fuse"
      }
    ],
    "snippets": [
      {
          "language": "lua",
          "path": "./src/snippets/snippets.code-snippets"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.53.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
